@model List<MyPortfolio.Models.TblExperiences>
<div class="experience" id="experience">
    <div class="container">
        <header class="section-header text-center wow zoomIn" data-wow-delay="0.1s">
            <p>My Resume</p>
            <h2>Working Experience</h2>
        </header>
        <div class="timeline">
            @for (int i = 0; i<Model.Count; i++)
            {


                <div class="timeline-item @(i%2 == 0 ? "left" : "right") wow slideIn@(i%2 == 0 ? "left" : "right")" data-wow-delay="0.1s">
                    <div class="timeline-text">
                        <div class="timeline-date">@Model[i].StartYear - @Model[i].EndYear</div>
                        <h2>@Model[i].Title</h2>
                        <h4>@Model[i].Company, @Model[i].Location</h4>
                        <p>
                            @Model[i].Description
                        </p>
                    </div>
                </div>






            }

        </div>
    </div>

</div>