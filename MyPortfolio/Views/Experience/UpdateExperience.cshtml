@model MyPortfolio.Models.TblExperiences
@{
    ViewBag.Title = "UpdateExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<br />
<h3>Update Experience Page</h3>

<form method="post">
    <input type="hidden" name="ExperienceId" value="@Model.ExperienceId" />
    <label>Start Year</label>
    <input type="text" name="StartYear" class="form-control" value="@Model.StartYear" />
    <br />
    <label>End Year</label>
    <input type="text" name="EndYear" class="form-control" value="@Model.EndYear" />
    <br />
    <label>Title</label>
    <input type="text" name="Title" class="form-control" value="@Model.Title" />
    <br />
    <label>Description</label>
    <textarea maxlength="300" name="Description" class="form-control" value="@Model.Description"></textarea>
    <br />
    <label>Company</label>
    <input type="text" name="Company" class="form-control" value="@Model.Company" />
    <br />
    <label>Location</label>
    <input type="text" name="Location" class="form-control" value="@Model.Location" />
    <br />
    <button class="btn btn-outline-primary">Update</button>
    



</form>



